      "rdfs:subClassOf": "untp-core:ConformityScheme"
    },
    {
      "@id": "_SustainabilityVocabulary:ConformityCriterion",
      "@type": "rdfs:Class",
      "rdfs:comment": "A conformity criterion under the scheme. Note that the same criterion ID may be reused across multiple schemes (particularly across different versions of the same scheme).",
      "rdfs:label": "ConformityCriterion",
      "rdfs:subClassOf": "untp-core:Criterion"
    },
    {
      "@id": "_SustainabilityVocabulary:LevelCode",
      "@type": "rdfs:Class",
      "rdfs:comment": "A list of performance level codes that may be assigned to criterion. Conformity at a higher level requires conformity agaisnt all criteria at lower levels.",
      "rdfs:label": "LevelCode"
    },
    {
      "@id": "_SustainabilityVocabulary:TagCode",
