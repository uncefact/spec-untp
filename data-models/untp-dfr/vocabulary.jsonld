{
  "@context": {
    "untp-dfr": "https://test.uncefact.org/vocabulary/untp/dfr/working/",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "owl": "http://www.w3.org/2002/07/owl#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "schema": "https://schema.org/",
    "untp-core": "https://test.uncefact.org/vocabulary/untp/core/working/"
  },
  "@graph": [
    {
      "@id": "untp-dfr:DigitalFacilityRecord",
      "@type": "rdfs:Class",
      "rdfs:comment": "A digital verifiable credential holding facility performance information.",
      "rdfs:label": "DigitalFacilityRecord",
      "rdfs:subClassOf": "untp-core:VerifiableCredential"
    },
    {
      "@id": "untp-dfr:FacilityRecord",
      "@type": "rdfs:Class",
      "rdfs:comment": "The physical site (eg farm or factory) where the product or materials was produced. Including the facility sustainability performance record.",
      "rdfs:label": "FacilityRecord"
    },
    {
      "@id": "untp-dfr:RenderTemplate2024",
      "@type": "rdfs:Class",
      "rdfs:comment": "A single template format focused render method--where the content/media type decision becomes secondary (and is expressed separately).See https://github.com/w3c-ccg/vc-render-method/issues/9",
      "rdfs:label": "RenderTemplate2024",
      "rdfs:subClassOf": "untp-core:RenderTemplate2024"
    },
    {
      "@id": "untp-dfr:WebRenderingTemplate2022",
      "@type": "rdfs:Class",
      "rdfs:comment": "A simple (though non-standard) embedded template.",
      "rdfs:label": "WebRenderingTemplate2022",
      "rdfs:subClassOf": "untp-core:WebRenderingTemplate2022"
    },
    {
      "@id": "untp-dfr:id",
      "schema:rangeIncludes": {
        "@id": "xsd:string"
      },
      "@type": [
        "rdf:Property",
        "owl:DatatypeProperty"
      ],
      "schema:domainIncludes": [
        {
          "@id": "untp-dfr:FacilityRecord"
        }
      ],
      "rdfs:comment": [
        "The globally unique ID of the facility as URI, ideally as a resolvable URL."
      ],
      "rdfs:label": "id"
    },
    {
      "@id": "untp-dfr:name",
      "schema:rangeIncludes": {
        "@id": "schema:name"
      },
      "@type": [
        "rdf:Property",
        "owl:ObjectProperty"
      ],
      "schema:domainIncludes": [
        {
          "@id": "untp-dfr:FacilityRecord"
        },
        {
          "@id": "untp-dfr:RenderTemplate2024"
        },
        {
          "@id": "untp-dfr:WebRenderingTemplate2022"
        }
      ],
      "rdfs:comment": [
        "The registered name of the facility within the identifier scheme.  ",
        "Human facing display name for selection",
        "Human facing display name for template"
      ],
      "rdfs:label": "name"
    },
    {
      "@id": "untp-dfr:registeredId",
      "schema:rangeIncludes": {
        "@id": "xsd:string"
      },
      "@type": [
        "rdf:Property",
        "owl:DatatypeProperty"
      ],
      "schema:domainIncludes": [
        {
          "@id": "untp-dfr:FacilityRecord"
        }
      ],
      "rdfs:comment": [
        "The registration number (alphanumeric) of the facility within the identifier scheme. Unique within the register."
      ],
      "rdfs:label": "registeredId"
    },
    {
      "@id": "untp-dfr:idScheme",
      "schema:rangeIncludes": {
        "@id": "untp-core:IdentifierScheme"
      },
      "@type": [
        "rdf:Property",
        "owl:ObjectProperty"
      ],
      "schema:domainIncludes": [
        {
          "@id": "untp-dfr:FacilityRecord"
        }
      ],
      "rdfs:comment": [
        "The ID scheme of the facility. eg a GS1 GLN or a National land registry scheme. If self issued then use the party ID of the facility owner. "
      ],
      "rdfs:label": "idScheme"
    },
    {
      "@id": "untp-dfr:description",
      "schema:rangeIncludes": {
        "@id": "schema:description"
      },
      "@type": [
        "rdf:Property",
        "owl:ObjectProperty"
      ],
      "schema:domainIncludes": [
        {
          "@id": "untp-dfr:FacilityRecord"
        }
      ],
      "rdfs:comment": [
        "An optional short description of the facility"
      ],
      "rdfs:label": "description"
    },
    {
      "@id": "untp-dfr:countryOfOperation",
      "schema:rangeIncludes": {
        "@id": "untp-dfr:countryCode"
      },
      "@type": [
        "rdf:Property",
        "owl:ObjectProperty"
      ],
      "schema:domainIncludes": [
        {
          "@id": "untp-dfr:FacilityRecord"
        }
      ],
      "rdfs:comment": [
        "The country in which this facility is operating."
      ],
      "rdfs:label": "countryOfOperation"
    },
    {
      "@id": "untp-dfr:processCategory",
      "schema:rangeIncludes": {
        "@id": "untp-core:Classification"
      },
      "@type": [
        "rdf:Property",
        "owl:ObjectProperty"
      ],
      "schema:domainIncludes": [
        {
          "@id": "untp-dfr:FacilityRecord"
        }
      ],
      "rdfs:comment": [
        "The industrial or production processes performed by this facility. Example unstats.un.org/isic/1030."
      ],
      "rdfs:label": "processCategory"
    },
    {
      "@id": "untp-dfr:operatedByParty",
      "schema:rangeIncludes": {
        "@id": "untp-core:Party"
      },
      "@type": [
        "rdf:Property",
        "owl:ObjectProperty"
      ],
      "schema:domainIncludes": [
        {
          "@id": "untp-dfr:FacilityRecord"
        }
      ],
      "rdfs:comment": [
        "The Party entity responsible for operating the facility.An object structure that uniquely identifies the party within a known party identification scheme. "
      ],
      "rdfs:label": "operatedByParty"
    },
    {
      "@id": "untp-dfr:facilityAlsoKnownAs",
      "schema:rangeIncludes": {
        "@id": "untp-core:Facility"
      },
      "@type": [
        "rdf:Property",
        "owl:ObjectProperty"
      ],
      "schema:domainIncludes": [
        {
          "@id": "untp-dfr:FacilityRecord"
        }
      ],
      "rdfs:comment": [
        "An optional list of other registered identifiers for this facility - eg GLNs or other schemes."
      ],
      "rdfs:label": "facilityAlsoKnownAs"
    },
    {
      "@id": "untp-dfr:locationInformation",
      "schema:rangeIncludes": {
        "@id": "untp-core:Location"
      },
      "@type": [
        "rdf:Property",
        "owl:ObjectProperty"
      ],
      "schema:domainIncludes": [
        {
          "@id": "untp-dfr:FacilityRecord"
        }
      ],
      "rdfs:comment": [
        "Geo-location information for this facility as a resolvable geographic area (a Plus Code), and/or a geo-located point (latitude / longitude), and/or a defined boundary (GeoJSON Polygon)."
      ],
      "rdfs:label": "locationInformation"
    },
    {
      "@id": "untp-dfr:address",
      "schema:rangeIncludes": {
        "@id": "untp-core:Address"
      },
      "@type": [
        "rdf:Property",
        "owl:ObjectProperty"
      ],
      "schema:domainIncludes": [
        {
          "@id": "untp-dfr:FacilityRecord"
        }
      ],
      "rdfs:comment": [
        "The Postal address of the location."
      ],
      "rdfs:label": "address"
    },
    {
      "@id": "untp-dfr:conformityClaim",
      "schema:rangeIncludes": {
        "@id": "untp-core:Claim"
      },
      "@type": [
        "rdf:Property",
        "owl:ObjectProperty"
      ],
      "schema:domainIncludes": [
        {
          "@id": "untp-dfr:FacilityRecord"
        }
      ],
      "rdfs:comment": [
        "A list of conformity declarations against specific criteria from recognised standards or regulations. "
      ],
      "rdfs:label": "conformityClaim"
    },
    {
      "@id": "untp-dfr:mediaQuery",
      "schema:rangeIncludes": {
        "@id": "xsd:string"
      },
      "@type": [
        "rdf:Property",
        "owl:DatatypeProperty"
      ],
      "schema:domainIncludes": [
        {
          "@id": "untp-dfr:RenderTemplate2024"
        }
      ],
      "rdfs:comment": [
        "Media query as defined in https://www.w3.org/TR/mediaqueries-4/"
      ],
      "rdfs:label": "mediaQuery"
    },
    {
      "@id": "untp-dfr:template",
      "schema:rangeIncludes": {
        "@id": "xsd:string"
      },
      "@type": [
        "rdf:Property",
        "owl:DatatypeProperty"
      ],
      "schema:domainIncludes": [
        {
          "@id": "untp-dfr:RenderTemplate2024"
        },
        {
          "@id": "untp-dfr:WebRenderingTemplate2022"
        }
      ],
      "rdfs:comment": [
        "An inline template field for use cases where remote retrieval of a render method is suboptimal",
        "An inline template field "
      ],
      "rdfs:label": "template"
    },
    {
      "@id": "untp-dfr:url",
      "schema:rangeIncludes": {
        "@id": "xsd:string"
      },
      "@type": [
        "rdf:Property",
        "owl:DatatypeProperty"
      ],
      "schema:domainIncludes": [
        {
          "@id": "untp-dfr:RenderTemplate2024"
        }
      ],
      "rdfs:comment": [
        "URL for remotely hosted template"
      ],
      "rdfs:label": "url"
    },
    {
      "@id": "untp-dfr:mediaType",
      "schema:rangeIncludes": {
        "@id": "xsd:string"
      },
      "@type": [
        "rdf:Property",
        "owl:DatatypeProperty"
      ],
      "schema:domainIncludes": [
        {
          "@id": "untp-dfr:RenderTemplate2024"
        }
      ],
      "rdfs:comment": [
        "content/media type for (possible) conneg on the above URL and certainly for display use"
      ],
      "rdfs:label": "mediaType"
    },
    {
      "@id": "untp-dfr:digestMultibase",
      "schema:rangeIncludes": {
        "@id": "xsd:string"
      },
      "@type": [
        "rdf:Property",
        "owl:DatatypeProperty"
      ],
      "schema:domainIncludes": [
        {
          "@id": "untp-dfr:RenderTemplate2024"
        }
      ],
      "rdfs:comment": [
        "Used for resource integrity and/or validation of the inline `template`"
      ],
      "rdfs:label": "digestMultibase"
    }
  ]
}