        },
        {
          "@id": "untp-core:ConformityScheme"
        }
      ],
      "rdfs:comment": [
        "An optional short description of the party.",
        "An optional short description of the facility",
        "A textual description providing details about the product.",
        "A textual description of the declaration being made.",
        "A full text description of the criterion that clearly specifies how compliance is achieved and measured. ",
        "A textual description of the declaration being made.",
        "A textual description of the scope or purpose of this conformity attestation",
        "The description of this conformity scheme"
      ],
      "rdfs:label": "description"
    },
    {
      "@id": "untp-core:registrationCountry",
      "schema:rangeIncludes": {
        "@id": "untp-core:countryCode"
